{
  "name": "ludotheque-app",
  "version": "1.0.0",
  "description": "Application de gestion de ludoth√®que",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "test": "jest",
    "db:migrate": "node database/migrate.js up",
    "db:migrate:status": "node database/migrate.js status",
    "db:migrate:down": "node database/migrate.js down",
    "db:migrate:reset": "node database/migrate.js reset",
    "db:migrate:mark-all": "node database/migrate.js mark-all",
    "migrate": "node database/migrate.js up",
    "seed": "sequelize-cli db:seed:all",
    "init-parametrage": "node database/init-parametrage.js",
    "seed-modes-paiement": "node database/seeds/seedModesPaiement.js",
    "migrate-codes-reduction": "node database/migrations/addCodesReduction.js",
    "seed-codes-reduction": "node database/seeds/seedCodesReduction.js",
    "migrate-tarifs": "node database/migrations/addTarifsCotisation.js",
    "migrate-tarifs-par-defaut": "node database/migrations/addParDefautToTarifs.js",
    "migrate-email": "node database/migrations/addConfigurationsEmail.js",
    "migrate-sms": "node database/migrations/addConfigurationsSMS.js",
    "update-sms": "node database/migrations/updateConfigurationsSMS.js",
    "migrate-templates": "node database/migrations/addTemplatesMessages.js",
    "migrate-communications": "npm run migrate-email && npm run migrate-sms && npm run migrate-templates",
    "seed-templates": "node database/seeds/seedTemplatesMessages.js",
    "setup-communications": "npm run migrate-communications && npm run seed-templates",
    "seed-email-templates": "node database/seeds/seedEmailTemplates.js",
    "migrate-event-triggers": "node database/migrations/addEventTriggers.js",
    "seed-event-triggers": "node database/seeds/seedEventTriggers.js",
    "seed-templates-event-triggers": "node database/seeds/seedTemplatesForEventTriggers.js",
    "setup-event-triggers": "npm run migrate-event-triggers && npm run seed-event-triggers",
    "setup-complete-communications": "npm run migrate-templates && npm run migrate-event-triggers && npm run seed-templates-event-triggers && npm run seed-event-triggers",
    "check-triggers": "node database/check-triggers-status.js",
    "setup-email-config": "node database/seeds/seedEmailConfig.js",
    "test-email-system": "node database/diagnostics/test-email-system.js",
    "job-email-reminders": "node backend/jobs/emailReminders.js",
    "migrate-utf8mb4": "node database/migrations/convertUtf8mb4.js",
    "migrate-multi-sites": "node database/migrations/addMultiSites.js",
    "migrate-horaires-vacances": "node database/migrations/addHorairesVacances.js",
    "migrate-jeux-fields": "node database/migrations/addJeuxFields.js",
    "migrate-jeux-normalization": "node database/migrations/addJeuxNormalization.js",
    "migrate-jeux-data": "node database/migrations/migrateJeuxData.js",
    "setup-jeux-normalization": "npm run migrate-jeux-normalization && npm run migrate-jeux-data",
    "migrate-livres": "node database/migrations/addLivresNormalization.js",
    "migrate-emprunts-multi": "node database/migrations/addEmpruntsMultiTypes.js",
    "setup-livres": "npm run migrate-livres && npm run migrate-emprunts-multi",
    "migrate-films": "node database/migrations/addFilmsNormalization.js",
    "setup-films": "npm run migrate-films",
    "migrate-disques": "node database/migrations/addDisquesNormalization.js",
    "setup-disques": "npm run migrate-disques",
    "seed-sysadmin": "node database/seeds/seedSysadmin.js",
    "migrate-ip-autorisees": "node database/migrations/addIpAutorisees.js",
    "migrate-modules-actifs": "node database/migrations/addModulesActifs.js",
    "seed-all": "node database/seeds/seedAllData.js",
    "seed-all-force": "node database/seeds/seedAllData.js --force",
    "migrate-comptabilite": "node database/migrations/addComptabiliteAvancee.js",
    "generate-data": "node database/scripts/generateMassData.js",
    "migrate-llm": "node database/migrations/addConfigurationsLLM.js up",
    "migrate-thematiques": "node database/migrations/addThematiquesUnifiees.js up",
    "migrate-utilisateurs": "node database/migrations/refactorAdherentToUtilisateur.js"
  },
  "keywords": [
    "ludotheque",
    "jeux",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bwip-js": "^4.0.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "mysql2": "^3.6.5",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.17.2",
    "sequelize": "^6.35.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
